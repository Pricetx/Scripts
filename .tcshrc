setenv EDITOR nano
setenv LSCOLORS Ex
set prompt="%B%n%b@%m:%U%/%u%#"
set prompt2="%R?"
set prompt3="CORRECT> %B%R%b (yes|no|edit)?"
set complete=enhance
set correct=cmd
set autolist
bindkey "\e[3~" delete-char

alias ls ls -GF
alias dir ls -halGF
chmod a+rw `tty`
alias ggo 'screen -x gravitygunonly'
alias update 'sudo freebsd-update fetch install && sudo portsnap fetch update && sudo portupgrade -ai'
